onents/auth/Login.vue').default, beforeEnter: requireUnAuth },\n    { path: '/register', name: 'register', component: require('../components/auth/Register.vue').default, beforeEnter: requireUnAuth },\n    { path: '/forget', name:'forget', component: require('../components/auth/Forget.vue').default, beforeEnter: requireUnAuth },\n    { path: '/reset', name:'reset', component: require('../components/auth/Reset.vue').default, beforeEnter: requireAuth },\n\n    // Products\n    { path: '/products', name:'products', component: require('../components/products/List.vue').default },\n    { path: '/products/:name', name:'show-product', component: require('../components/products/Show.vue').default },\n\n    // My Profile\n    { path: '/cart', name:'cart', component: require('../components/orders/Cart.vue').default },\n    { path: '/track', name:'track', component: require('../components/orders/Track.vue').default },\n    { path: '/history', name:'history', component: require('../components/orders/History.vue').default },\n\n    // Pages\n    { path: '/videos', name:'videos', component: require('../components/pages/Videos.vue').default },\n    { path: '/complaints', name:'complaints', component: require('../components/pages/Complaints.vue').default },\n    { path: '/pricing', name:'pricing', component: require('../components/pages/Pricing.vue').default },\n    { path: '/contacts', name:'contacts', component: require('../components/pages/Contacts.vue').default },\n    { path: '/about', name:'about', component: require('../components/pages/About.vue').default },\n\n  ]\n})\n\n\nfunction requireAuth(to, from, next) {\n  //store.dispatch('fetchAccessToken');\n  //if (!store.state.accessToken) { next('/dashboard/login'); } \n  //else { next(); }\n  next();\n}\n\nfunction requireUnAuth(to, from, next) {\n  // store.dispatch('fetchAccessToken');\n  // if (store.state.accessToken) { next('/dashboard/explore'); } \n  // else { next(); }\n  next();\n}"]}]}